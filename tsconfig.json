{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "build",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "types": ["chrome"],
  "include": ["src/**/*"],
  "exclude": []
}
